    "# 16. How can you check if a string is a palindrome?\n",
    "txt = input(\"Enter a word for checking palindrome\")\n",
    "txt = txt.lower()\n",
    "words = txt[0::]\n",
    "revrs_words = txt[::-1]\n",
    "if words==revrs_words:\n",
    "    print(f\"{txt} is a palindrome \")\n",
    "else:\n",
    "    print(f\"{txt} is not a palindrome \")"
   ]
